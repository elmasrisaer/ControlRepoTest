{
  "sdkName": "Celitech",
  "apiName": "Celitech",
  "apiVersion": "1.1.88",
  "specFilePath": "swagger.yaml",
  "languages": ["java", "typescript"],
  "docs": ["snippets"],
  "integrations": {
    "enhancedApiSpec": {
      "provider": "redocly",
      "githubRepoName": "CelitechCoreAPIReference",
      "redocly": {
        "openapiPath": "swagger.yaml"
      }
    }
  },
  "customizations": {
    "devContainer": true,
    "generateEnv": true,
    "inferServiceNames": false,
    "includeOptionalSnippetParameters": false,
    "injectedModels": [],
    "license": {
      "type": "MIT"
    },
    "responseHeaders": false,
    "retry": {
      "enabled": true,
      "maxAttempts": 3,
      "retryDelay": 150
    },
    "environmentVariables": [
      {
        "name": "CLIENT_ID",
        "description": "Client ID parameter"
      },
      {
        "name": "CLIENT_SECRET",
        "description": "Client Secret parameter"
      }
    ]
  },
  "languageOptions": {
    "java": {
      "githubRepoName": "JavaRepoTest",
      "sdkVersion": "1.1.88",
      "liblabVersion": "2",
      "additionalConstructorParameters": [
        {
          "name": "clientId",
          "example": "client-id"
        },
        {
          "name": "clientSecret",
          "example": "client-secret"
        }
      ]
    }
  },
  "publishing": {
    "githubOrg": "elmasrisaer"
  }
}
